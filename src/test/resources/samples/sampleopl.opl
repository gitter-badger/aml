using CP;

range range_percent = 0..100;
range range_interfaces = 0..1;
range range_price = 0..400;
range range_size = 0..200;
range range_time = 0..100;

dvar int StorageSize in range_size;
dvar int MUP in range_percent;
dvar int ResponseTime in range_time;
dvar int StoragePrice in range_price;
dvar int Interface in range_interfaces;

subject to {
	C1: StoragePrice == StorageSize * 1;
	G1: ResponseTime <= 100;
}