Template AmazonS3 version 1.0
	Provider as Responder
	Metrics:
		percent: float [0..100]
		time: integer [0..100]
		interfaces: set {SOAP, REST}
		size: integer [0..200]
		price: integer [0..400]
		
AgreementTerms
	Service AWS-S3 availableAt. "aws.amazon.com/s3"
		GlobalDescription
			Interface: interfaces
			StorageSize: size
			StoragePrice: price

	MonitorableProperties
		global:
			MUP: percent
			ResponseTime: time
				
	GuaranteeTerms
		G1: Provider guarantees ResponseTime<=50;
                G2: Provider guarantees ResponseTime>50;

CreationConstraints
	C1: StoragePrice = StorageSize * 1; 
	
EndTemplate