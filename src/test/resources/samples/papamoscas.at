Template AmazonEC2withoutPricePolicy version 1.0
                Provider "Prov" as Responder;
                Metrics:
                    storageCost: integer [0..50000];
                    cores: integer [0..32];
                    ram: integer [0..244000];
                               
AgreementTerms
                Service AWS-S3 availableAt "aws.amazon.com/ec2"
                                GlobalDescription
                                            Cores: cores;
                                            RAM: ram;
                                               
                                               
                MonitorableProperties
                                global:
                                               StorageCost: storageCost;
                                                               
                GuaranteeTerms
                                G1: Provider guarantees StorageCost == Cores + RAM ;
                               
CreationConstraints
                //Cost constraints
                C41: StorageCost <= 250;
                //C41: Cost==CostHour*Usage;
EndTemplate